#mount point   fstype   device                    option             size hint
/reserved      hidden   /dev/block/mmcblk0_none   length=0           size_hint=100
/factory       ext4     /dev/block/mmcblk0p1      length=0           size_hint=256
/system        ext4     /dev/block/mmcblk0p2      length=0           size_hint=512
/cache         ext4     /dev/block/mmcblk0p3      length=0           size_hint=512
/config        ext4     /dev/block/mmcblk0p5      length=0           size_hint=16
/panic         raw      /dev/block/mmcblk0p6      length=0           size_hint=2
/media         vfat     /dev/block/mmcblk0p7      length=0           size_hint=128
/data          ext4     /dev/block/mmcblk0p8      length=-16384      size_hint=0
/logs          ext4     /dev/block/mmcblk0p9      length=0           size_hint=1024
/mnt/sdcard    vfat     /dev/block/mmcblk1p1      length=0           size_hint=-1
