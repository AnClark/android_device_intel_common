			component: /Route/ROUTEMGR/CRITERIA/TTY_DIRECTION_CRITERIA/TTY_DIRECTION/
				Downlink = 1
				Uplink = 1

		conf: Default
			component: /Route/ROUTEMGR/CRITERIA/TTY_DIRECTION_CRITERIA/TTY_DIRECTION/
